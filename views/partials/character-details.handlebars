<section class="card">
 
  <header>{{character_name}}{{character.avatar}}</header>
   
    <p>Race: {{race.race_name}}</p>
    <p>Class: {{class.class_name}}</p>
    <p>{{#each attributes as |attribute|}}
        {{> attribute}}
       {{/each}}
    </p>

    <div class="col-md-4">
      <button class="btn btn-sm btn-danger" data-id="{{character.id}}">DELETE</button>
    </div>

</section>